{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdej3"
		},
		"LS_DesjardinsLakehouse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_DesjardinsLakehouse'"
		},
		"LS_Desjardins_LAkeHouse_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Desjardins_LAkeHouse_Destination'"
		},
		"LS_testexfiltration2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_testexfiltration2'"
		},
		"Ls_testlinkservicestorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_testlinkservicestorage'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdej3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_testexfiltration2_connectionString": {
						"value": "[parameters('LS_testexfiltration2_connectionString')]"
					},
					"Ls_testlinkservicestorage_connectionString": {
						"value": "[parameters('Ls_testlinkservicestorage_connectionString')]"
					},
					"LS_DesjardinsLakehouse_connectionString": {
						"value": "[parameters('LS_DesjardinsLakehouse_connectionString')]"
					},
					"LS_Desjardins_LAkeHouse_Destination_connectionString": {
						"value": "[parameters('LS_Desjardins_LAkeHouse_Destination_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}